[html]
show_contexts = true
skip_covered = false

[paths]
_site-packages-to-src-mapping =
  .
  */lib/pypy*/site-packages
  */lib/python*/site-packages
  *\Lib\site-packages

[report]
exclude_also =
  ^\s*@pytest\.mark\.xfail
skip_covered = true
skip_empty = true
show_missing = true

[run]
branch = true
cover_pylib = false
parallel = true
plugins = Cython.Coverage
relative_files = true
source =
  .
source_pkgs =
  frozenlist
